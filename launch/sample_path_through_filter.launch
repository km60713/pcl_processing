<launch>
  <node pkg="nodelet" type="nodelet" name="pcl_manager" args="manager" output="screen" />


  <!-- Run a passthrough filter to clean NaNs -->
  <node pkg="nodelet" type="nodelet" name="zaxis_passthrough" args="load pcl/PassThrough pcl_manager" output="screen">
    <remap from="~input" to="/zed/point_cloud/cloud_registered" />
    <rosparam>
      filter_field_name: z
      filter_limit_min: -0.65
      <!-- filter_limit_max: -0.55 -->
      filter_limit_max: 1
      filter_limit_negative: False
    </rosparam>
  </node>

  <node pkg="nodelet" type="nodelet" name="yaxis_passthrough" args="load pcl/PassThrough pcl_manager" output="screen">
    <remap from="~input" to="/zaxis_passthrough/output" />
    <rosparam>
      filter_field_name: y
      filter_limit_min: -0.5
      filter_limit_max: 0.5
      filter_limit_negative: False
    </rosparam>
  </node>

 <node pkg="nodelet" type="nodelet" name="xaxis_passthrough" args="load pcl/PassThrough pcl_manager" output="screen">
    <remap from="~input" to="/yaxis_passthrough/output" />
    <rosparam>
      filter_field_name: x
      filter_limit_min: 0
      filter_limit_max: 10
      filter_limit_negative: False
    </rosparam>
  </node>
  
</launch>
